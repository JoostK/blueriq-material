{
  "name": "blueriq-material",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "ng:version": "ng --version",
    "start": "ng serve --open --host 0.0.0.0 --proxy-config proxy.conf.json",
    "start:nolive": "ng serve --open --host 0.0.0.0 --proxy-config proxy.conf.json --live-reload false --watch false",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "yarn tslint && yarn sass-lint",
    "tslint": "ng lint",
    "sass-lint": "sass-lint --verbose --config sass-lint.yml src/**/*.scss",
    "verify": "ng test --watch false --code-coverage true",
    "verify:all": "yarn verify && yarn tslint && yarn sass-lint",
    "e2e": "yarn protractor e2e/protractor.conf.js",
    "e2e-dev": "yarn webdriver-manager update && protractor e2e/protractor-dev.conf.js",
    "docs": "compodoc --config compodoc.json",
    "version:increment": "npm version --allow-same-version"
  },
  "private": true,
  "engines": {
    "yarn": "^1.4.0"
  },
  "dependencies": {
    "@angular/animations": "^6.0.1",
    "@angular/cdk": "^6.0.1",
    "@angular/common": "^6.0.1",
    "@angular/compiler": "^6.0.1",
    "@angular/core": "^6.0.1",
    "@angular/flex-layout": "^6.0.0-beta.16",
    "@angular/forms": "^6.0.1",
    "@angular/http": "^6.0.1",
    "@angular/material": "6.3.2",
    "@angular/material-moment-adapter": "^6.2.1",
    "@angular/platform-browser": "^6.0.1",
    "@angular/platform-browser-dynamic": "^6.0.1",
    "@angular/router": "^6.0.1",
    "@blueriq/angular": "^0.4.0-alpha.6",
    "@blueriq/core": "^0.4.0-alpha.6",
    "@ngrx/effects": "6.0.1",
    "@ngrx/store": "6.0.1",
    "@ngrx/store-devtools": "6.0.1",
    "core-js": "^2.5.4",
    "hammerjs": "^2.0.8",
    "material-design-icons": "^3.0.1",
    "moment": "^2.22.2",
    "ng-pick-datetime": "6.0.5",
    "ng-pick-datetime-moment": "1.0.6",
    "ng2-file-upload": "^1.3.0",
    "roboto-fontface": "^0.9.0",
    "rxjs": "^6.1.0",
    "rxjs-compat": "^6.1.0",
    "web-animations-js": "^2.3.1",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.1",
    "@angular/cli": "~6.0.1",
    "@angular/compiler-cli": "^6.0.1",
    "@angular/language-service": "^6.0.1",
    "@compodoc/compodoc": "^1.1.3",
    "@types/jasmine": "~2.8.7",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.3.0",
    "jasmine-core": "~3.1.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^2.0.5",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "2.0.1",
    "karma-jasmine": "1.1.2",
    "karma-jasmine-html-reporter": "1.1.0",
    "karma-junit-reporter": "^1.2.0",
    "protractor": "5.3.2",
    "protractor-jasmine2-html-reporter": "^0.0.7",
    "sass-lint": "~1.12.1",
    "ts-node": "~6.0.3",
    "tslint": "~5.10.0",
    "typescript": "~2.7.2",
    "webdriver-manager": "^12.1.0"
  }
}
